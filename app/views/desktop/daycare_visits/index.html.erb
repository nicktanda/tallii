    <div class="flex items-center space-x-4">
      <div class="font-bold text-2xl">Daycare</div>
      <div class="w-full p-4 flex items-center space-x-4">
        <div class="flex items-center space-x-2">
          <%= link_to desktop_daycare_visits_path(date: @date - 1.day) do %>
            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-8 h-8">
              <path stroke-linecap="round" stroke-linejoin="round" d="m11.25 9-3 3m0 0 3 3m-3-3h7.5M21 12a9 9 0 1 1-18 0 9 9 0 0 1 18 0Z" />
            </svg>
          <% end %>
          <%= link_to desktop_daycare_visits_path(date: @date + 1.day) do %>
            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-8 h-8">
              <path stroke-linecap="round" stroke-linejoin="round" d="m12.75 15 3-3m0 0-3-3m3 3h-7.5M21 12a9 9 0 1 1-18 0 9 9 0 0 1 18 0Z" />
            </svg>
          <% end %>
        </div>
        <div class="font-bold text-xl"><%= @date.strftime('%d %b %Y') %></div>
      </div>
    </div>
    <div class="bg-white border border-gray-200 rounded-lg">
      <div class="flex">
        <div class="w-full border">
          <div class="font-bold text-center p-4 text-xl">Daycare Dogs</div>
        </div>
        <div class="w-full border">
          <div class="font-bold text-center p-4 text-xl">Currently In</div>
        </div>
        <div class="w-full border">
          <div class="font-bold text-center p-4 text-xl">Checked Out</div>
        </div>
