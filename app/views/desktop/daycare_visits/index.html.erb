                        <% if daycare_visit.is_a?(DaycareVisit) && daycare_visit.pet.images.any? %>
                          <img src="<%= url_for(daycare_visit.pet.images.order(:created_at).last.image) %>" alt="<%= daycare_visit.pet.name %>" class="rounded-full h-6 w-6">
                        <% end %>
                        <div class="font-bold">
                          <%= daycare_visit.is_a?(DaycareVisit) ? daycare_visit.pet.name : daycare_visit.pet_name %>
                        </div>
                        <% if daycare_visit.user&.colour.present? %>
                          <div class="rounded-full h-2 w-2" style="background-color: <%= daycare_visit.user.colour %>;"></div>
                        <% end %>
                      </div>
                    </td>
                    <% if daycare_visit.duration == "half_day" %>
                      <td class="text-sm"><%= daycare_visit.formatted_duration %>(5hrs)</td>
                    <% else %>
                      <td class="text-sm"><%= daycare_visit.formatted_duration %>(10hrs)</td>
