                              <div class="space-y-2">
                                <div class="bg-white rounded-xl p-4 space-y-2">
                                  <div>Scheduled Pet:</div>
                                  <div class="bg-green-300 border border-green-500 p-4 rounded-xl inline-flex space-x-2 w-fit">
                                    <% if daycare_visit.respond_to?(:pet) && daycare_visit.pet.images.any? %>
                                      <img src="<%= url_for(daycare_visit.pet.images.order(:created_at).last.image) %>" alt="<%= daycare_visit.pet.name %>" class="rounded-2xl h-20 w-20">
                                    <% end %>
                                    <div>
                                      <div class="font-bold text-lg"><%= daycare_visit.is_a?(DaycareVisit) ? daycare_visit.pet.name : daycare_visit.pet_name %></div>
                                      <% if daycare_visit.is_a?(DaycareVisit) %>
                                        <div><%= daycare_visit.pet.breed %></div>
                                        <div><%= daycare_visit.pet.gender.humanize %></div>
                                      <% end %>
                                    </div>
                                  </div>
                                  <div class="flex justify-between">
                                    <div>Customer:</div>
                                    <div>
                                      <% if daycare_visit.is_a?(DaycareVisit) %>
                                        <%= link_to desktop_user_edit_path(daycare_visit.pet.user), class: "focus:outline-none" do %>
                                          <div class="text-blue-500"><%= daycare_visit.pet.user.full_name %></div>
                                        <% end %>
                                      <% else %>
                                        <div class="text-blue-500"><%= daycare_visit.owner_name %></div>
                                      <% end %>
                                    </div>
                                  </div>
                                  <div class="flex justify-between">
                                    <div>Date:</div>
                                    <div><%= daycare_visit.date.strftime('%d %b %Y') %></div>
                                  </div>
                                  <div class="flex justify-between">
                                    <div>Time:</div>
                                    <div><%= daycare_visit.time.strftime('%H:%M %p') %></div>
                                  </div>
                                  <div class="flex justify-between items-center">
                                    <div>Duration:</div>
                                    <%= f.select(:duration, options_for_select([["Full Day", "full_day"], ["Half Day", "half_day"]], selected: daycare_visit.duration), { prompt: 'Select' }, { class: "bg-white text-black border border-gray-300 rounded-lg p-2" }) %>
                                  </div>
                                </div>
                                <div class="bg-white rounded-xl p-4 space-y-2">
                                  <% if daycare_visit.is_a?(DaycareVisit) %>
                                    <div>
                                      <%= f.label :notes, "Groomer Notes" %>
                                      <%= f.text_area :notes, class: "bg-white text-black border border-gray-300 w-full rounded-lg p-4", rows: 3 %>
                                    </div>
                                  <% else %>
                                    <div>
                                      <%= f.label :pet_notes, "Groomer Notes" %>
                                      <%= f.text_area :pet_notes, class: "bg-white text-black border border-gray-300 w-full rounded-lg p-4", rows: 3 %>
                                    </div>
                                  <% end %>
                                  <div>
                                    <%= f.label :status, "Status" %>
                                    <%= f.select(:status, options_for_select([["Pending", "pending"], ["Confirmed", "confirmed"], ["In Progress", "in_progress"], ["Completed", "completed"], ["Missed Appointment", "missed_appointment"]], selected: daycare_visit.status), { prompt: 'Select' }, { required: true, class: "bg-gray-100 border border-gray-100 text-black w-full rounded-lg p-4" }) %>
                                  </div>
                                </div>
                                <div class="p-2 w-full flex justify-center">
                                  <button type="submit" class="bg-blue-500 text-white font-bold p-2 rounded-lg flex items-center justify-center">
                                    Update Daycare Visit
                                  </button>
                                </div>
                              </div>
                            <% end %>
                          </div>
                        </div>
